<script>


    $('.intro').click(function () {

        var intro = introJs();
        intro.setOptions({
            steps: [
                {
                    intro: "<p class='h4 blockquote'>Welcome to iStack Tutorial</p>"
                },

                {
                    element: document.querySelector('.add-element'),
                    intro: "<p class='h4 blockquote'>Add New Element</p>",
